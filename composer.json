{
  "name": "dumb/dumb",
  "description": "A Dumb Package",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Dumb\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DumbTest\\": "test"
    }
  },
  "require-dev": {
    "overtrue/phplint": "^9.6",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^12.2"
  },
  "config": {
    "cache-read-only": true,
    "sort-packages": true
  },
  "scripts": {
    "test": [
      "phplint",
      "phpunit $COVERAGE_OPTIONS"
    ],
    "scan": [
      "phpstan"
    ]
  }
}
